{
  "printWidth": 100,
  "overrides": [
    {
      "files": ["test/*.js", "test/**/*.js"],
      "options": {
        "printWidth": 120
      }
    },
    {
      "files": "*.sol",
      "options": {
        "printWidth": 120,
        "tabWidth": 4,
        "useTabs": false,
        "singleQuote": false,
        "bracketSpacing": false,
        "explicitTypes": "always"
      }
    }
  ],
  "bracketSpacing": false
}
